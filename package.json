{
    "name": "click-to-copy",
    "version": "1.0.0",
    "description": "A library for component wrapping labels and h1 so that we can click to copy the their text.",
    "main": "dist/index.js",
    "scripts": {
        "lint": "eslint src",
        "lint:fix": "eslint src --ext .js --fix",
        "test": "jest",
        "validate": "npm-run-all --parallel lint:fix test build",
        "prebuild": "rimraf dist",
        "build": "npm-run-all --parallel build:*",
        "build:main": "babel --out-dir dist --ignore __tests__/ src",
        "build:umd": "webpack --output-filename index.umd.js -d",
        "build:umd.min": "webpack --output-filename index.umd.min.js -p"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/diwakersurya/click-to-copy.git"
    },
    "keywords": [
        "react-component",
        "click-to-copy",
        "clickToCopy",
        "copy",
        "select",
        "copy",
        "copy-text",
        "text-copy"
    ],
    "author": "Diwaker Singh",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/diwakersurya/click-to-copy/issues"
    },
    "homepage": "https://github.com/diwakersurya/click-to-copy#readme",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^10.0.1",
        "babel-jest": "^23.6.0",
        "babel-loader": "^7.0.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^5.12.0",
        "eslint-config-kentcdodds": "^14.0.4",
        "eslint-config-react-app": "^3.0.6",
        "eslint-plugin-flowtype": "^3.2.1",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-jsx-a11y": "^6.1.2",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.3",
        "husky": "^1.3.1",
        "jest": "^23.6.0",
        "jest-dom": "^3.0.0",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.15.3",
        "react-test-renderer": "^16.7.0",
        "react-testing-library": "^5.4.4",
        "rimraf": "^2.6.3",
        "webpack": "^4.28.4",
        "webpack-cli": "^3.2.1",
        "react": "^16.7.0",
        "react-dom": "^16.7.0"
    },
    "peerDependencies": {
        "react": "*",
        "react-dom":"*"
    }
}
